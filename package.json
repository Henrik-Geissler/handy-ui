{
  "name": "handy-ui",
  "version": "1.0.28",
  "description": "A Collection of useful React Components",
  "author": "Henrik GeiÃŸler",
  "license": "MIT",
  "homepage": "https://github.com/Henrik-Geissler/handy-ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Henrik-Geissler/handy-ui.git"
  },
  "keywords": [
    "react",
    "material-ui",
    "ui"
  ],
  "bugs": {
    "url": "https://github.com/Henrik-Geissler/handy-ui/issues"
  },
  "scripts": {
    "build": "rm -rf dist && tsc",
    "ci:build": "tsc",
    "ci:format": "./node_modules/.bin/prettier --write \".\"",
    "ci:upgrade": "yarn upgrade --latest",
    "ci:lint": "./node_modules/.bin/eslint --fix \".\"",
    "ci:patch": "yarn version --patch",
    "ci:test": "jest --coverage",
    "dev": "yarn watch",
    "prepublish": "yarn run build",
    "test": "jest",
    "watch": "mkdir \"`pwd`\"/secrets; echo \"cd \"`pwd`\";yarn watch:build;exit;\" > \"`pwd`\"/secrets/watchBuild.command; chmod +x \"`pwd`\"/secrets/watchBuild.command; echo \"cd \"`pwd`\";yarn watch:test;exit;\" > \"`pwd`\"/secrets/watchTest.command; chmod +x \"`pwd`\"/secrets/watchTest.command; open \"`pwd`\"/secrets/watchTest.command; open \"`pwd`\"/secrets/watchBuild.command;",
    "watch:build": "tsc -w",
    "watch:test": "jest --coverage --watch ./src"
  },
  "main": "dist/main.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "dependencies": {
    "react": "^17.0.1",
    "@types/material-ui":"^0.21.8",
    "@material-ui/core":"^4.11.0",
    "notistack":"^1.0.3",
    "next": "^9.5.3"
  },
  "devDependencies": {
    "@getify/eslint-plugin-proper-arrows": "^10.0.0",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.19",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-canonical": "^25.0.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-alphabetize": "^1.1.1",
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-better-styled-components": "^1.1.2",
    "eslint-plugin-compat": "^3.8.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.13",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-no-secrets": "^0.6.8",
    "eslint-plugin-no-unsanitized": "^3.1.4",
    "eslint-plugin-notice": "^0.9.10",
    "eslint-plugin-optimize-regex": "^1.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-sort-class-members": "^1.8.0",
    "eslint-plugin-sort-destructure-keys": "^1.3.5",
