    "eslint-plugin-this": "^0.2.3",
    "eslint-plugin-toplevel": "^1.0.1",
    "eslint-plugin-type-graphql": "^0.3.0",
    "eslint-plugin-typescript-sort-keys": "^1.5.0",
    "eslint-plugin-woke": "^1.0.0",
    "eslint-plugin-xss": "^0.1.10",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "react-dom": "^17.0.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  }
}
